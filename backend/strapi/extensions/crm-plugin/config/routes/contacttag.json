{
  "routes": [
    {
      "method": "GET",
      "path": "/contacttags",
      "handler": "contacttag.find"
    },
    {
      "method": "GET",
      "path": "/contacttags/count",
      "handler": "contacttag.count"
    },
    {
      "method": "GET",
      "path": "/contacttags/:id",
      "handler": "contacttag.findOne"
    },
    {
      "method": "POST",
      "path": "/contacttags",
      "handler": "contacttag.create"
    },
    {
      "method": "PUT",
      "path": "/contacttags/:id",
      "handler": "contacttag.update"
    },
    {
      "method": "DELETE",
      "path": "/contacttags/:id",
      "handler": "contacttag.delete"
    }
  ]
}
