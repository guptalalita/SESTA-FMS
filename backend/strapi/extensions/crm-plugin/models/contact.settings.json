{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "contacts",
  "info": {
    "name": "contact"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "phone_other": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "email_other": {
      "type": "email"
    },
    "address_1": {
      "type": "text"
    },
    "address_2": {
      "type": "text"
    },
    "city": {
      "type": "string"
    },
    "pincode": {
      "type": "string"
    },
    "contact_type": {
      "type": "string",
      "required": true
    },
    "country": {
      "plugin": "crm-plugin",
      "model": "country"
    },
    "villages": {
      "plugin": "crm-plugin",
      "collection": "village",
      "via": "contacts",
      "dominant": true
    },
    "state": {
      "plugin": "crm-plugin",
      "model": "state"
    },
    "district": {
      "plugin": "crm-plugin",
      "model": "district"
    },
    "individual": {
      "plugin": "crm-plugin",
      "model": "individual",
      "via": "contact"
    },
    "activityassignees": {
      "plugin": "crm-plugin",
      "collection": "activityassignee",
      "via": "contact"
    },
    "contacttags": {
      "plugin": "crm-plugin",
      "collection": "contacttag",
      "via": "contact"
    },
    "activitytypes": {
      "plugin": "crm-plugin",
      "collection": "activitytype",
      "via": "contact"
    },
    "block": {
      "type": "string"
    },
    "gp": {
      "type": "string"
    },
    "shareinformation": {
      "model": "shareinformation",
      "via": "contact"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "contact"
    },
    "loan_model": {
      "model": "loan-model",
      "via": "fpo"
    },
    "loan_app_approvedby": {
      "model": "loan-application",
      "via": "approved_by"
    },
    "organization": {
      "plugin": "crm-plugin",
      "model": "organization",
      "via": "contact"
    },
    "org_vos": {
      "plugin": "crm-plugin",
      "collection": "organization",
      "via": "vos"
    },
    "loan_applications": {
      "collection": "loan-application",
      "via": "contact"
    },
    "creator_id": {
      "plugin": "crm-plugin",
      "model": "contact"
    }
  }
}
