{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "contacts",
  "info": {
    "name": "contact"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "unique": true
    },
    "phone_other": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "email_other": {
      "type": "email"
    },
    "city": {
      "type": "string"
    },
    "contact_type": {
      "type": "string",
      "required": true
    },
    "individual": {
      "plugin": "crm-plugin",
      "model": "individual",
      "via": "contact"
    },
    "activityassignees": {
      "plugin": "crm-plugin",
      "collection": "activityassignee",
      "via": "contact"
    },
    "contacttags": {
      "plugin": "crm-plugin",
      "collection": "contacttag",
      "via": "contact"
    },
    "activitytypes": {
      "plugin": "crm-plugin",
      "collection": "activitytype",
      "via": "contact"
    },
    "shareinformation": {
      "via": "contact",
      "model": "shareinformation"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "contact"
    },
    "loan_app_approvedby": {
      "via": "approved_by",
      "model": "loan-application"
    },
    "organization": {
      "plugin": "crm-plugin",
      "model": "organization",
      "via": "contact"
    },
    "org_vos": {
      "plugin": "crm-plugin",
      "collection": "organization",
      "via": "vos"
    },
    "org_fpo": {
      "plugin": "crm-plugin",
      "model": "organization",
      "via": "fpo"
    },
    "loan_applications": {
      "via": "contact",
      "collection": "loan-application"
    },
    "creator_id": {
      "plugin": "crm-plugin",
      "model": "contact"
    },
    "addresses": {
      "plugin": "crm-plugin",
      "collection": "address",
      "via": "contact"
    },
    "pg": {
      "plugin": "crm-plugin",
      "model": "tag"
    }
  }
}
