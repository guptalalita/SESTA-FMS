{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "activities",
  "info": {
    "name": "activity"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "start_datetime": {
      "type": "datetime"
    },
    "end_datetime": {
      "type": "datetime"
    },
    "description": {
      "type": "richtext"
    },
    "activitytype": {
      "via": "activities",
      "plugin": "crm-plugin",
      "model": "activitytype"
    },
    "activityassignees": {
      "plugin": "crm-plugin",
      "collection": "activityassignee",
      "via": "activity"
    },
    "loan_application_installment": {
      "model": "loan-application-installments"
    },
    "loan_application_task": {
      "model": "loan-application-tasks"
    },
    "unit": {
      "type": "string"
    }
  }
}
