{
  "connection": "default",
  "collectionName": "shgs",
  "info": {
    "name": "shg"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "text"
    },
    "person_incharge": {
      "type": "string"
    },
    "villages": {
      "via": "shgs",
      "collection": "village"
    },
    "state": {
      "model": "state"
    },
    "district": {
      "model": "district"
    },
    "village_organization": {
      "via": "shgs",
      "model": "village-organization"
    }
  }
}
