{
  "connection": "default",
  "collectionName": "loanmodels",
  "info": {
    "name": "loanmodel"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "product_name": {
      "type": "string",
      "required": true
    },
    "specification": {
      "type": "text",
      "required": true
    },
    "loan_amount": {
      "type": "decimal",
      "private": false,
      "required": true
    },
    "duration": {
      "type": "integer",
      "required": true
    },
    "emi": {
      "type": "decimal",
      "required": true
    },
    "fpo": {
      "model": "contact"
    },
    "emidetails": {
      "via": "loan_model",
      "collection": "emidetails"
    },
    "loantasks": {
      "via": "loan_model",
      "collection": "loantask"
    }
  }
}
