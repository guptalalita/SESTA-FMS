{
  "kind": "collectionType",
  "collectionName": "loan_models",
  "info": {
    "name": "Loan Model"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "product_name": {
      "type": "string",
      "required": true
    },
    "specification": {
      "type": "string",
      "required": true
    },
    "loan_amount": {
      "type": "decimal",
      "unique": false,
      "required": true
    },
    "duration": {
      "type": "integer",
      "required": true
    },
    "emi": {
      "type": "decimal",
      "required": true
    },
    "fpo": {
      "via": "loan_model",
      "plugin": "crm-plugin",
      "model": "contact"
    },
    "loan_applications": {
      "via": "loan_model",
      "collection": "loan-application"
    },
    "emidetails": {
      "collection": "emidetails",
      "via": "loan_model"
    },
    "loantasks": {
      "collection": "loantask",
      "via": "loan_model"
    }
  }
}
