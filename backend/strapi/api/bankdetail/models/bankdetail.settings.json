{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "bankdetails",
  "info": {
    "name": "bankdetail"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "account_name": {
      "type": "string",
      "required": true
    },
    "account_no": {
      "type": "string",
      "required": true
    },
    "bank_name": {
      "type": "string",
      "required": true
    },
    "branch": {
      "type": "string",
      "required": true
    },
    "ifsc_code": {
      "type": "string",
      "required": true
    },
    "organization": {
      "plugin": "crm-plugin",
      "via": "bankdetail",
      "model": "organization"
    }
  }
}
