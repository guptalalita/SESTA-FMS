{
  "connection": "default",
  "collectionName": "states",
  "info": {
    "name": "master state"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "is_active": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "master_districts": {
      "collection": "district",
      "via": "master_state"
    },
    "villages": {
      "collection": "village",
      "via": "state"
    },
    "Identifier": {
      "type": "string",
      "unique": true
    },
    "Abbreviation": {
      "type": "string",
      "unique": true
    },
    "country": {
      "model": "country",
      "via": "master_states"
    }
  }
}
