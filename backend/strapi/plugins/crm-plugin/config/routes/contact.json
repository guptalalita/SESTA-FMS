{
  "routes": [
    {
      "method": "GET",
      "path": "/contact",
      "handler": "contact.find"
    },
    {
      "method": "GET",
      "path": "/contact/:id",
      "handler": "contact.findOne"
    },
    {
      "method": "POST",
      "path": "/contact",
      "handler": "contact.create"
    },
    {
      "method": "PUT",
      "path": "/contact/:id",
      "handler": "contact.update"
    },
    {
      "method": "DELETE",
      "path": "/contact/:id",
      "handler": "contact.delete"
    }
  ]
}
